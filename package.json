{
  "name": "enjoyer-theme",
  "publisher": "nikkymen",
  "displayName": "Enjoyer Theme",
  "description": "Light color theme inspired by QtCreator",
  "version": "0.0.4",
  "icon": "images/logo.png",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nikkymen/vscode-enjoyer-theme"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Themes"
  ],
  "extensionDependencies": [
    "SteveBushResearch.BuildOutputColorizer"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Enjoyer Theme",
        "uiTheme": "vs",
        "path": "./themes/Enjoyer Light.json"
      }
    ],
    "configurationDefaults": {
      "editor.tokenColorCustomizations": {
        "textMateRules": []
      },
      "editor.bracketPairColorization.enabled": false
    }
  }
}